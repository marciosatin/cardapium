{
    "name": "marciosatin/cardapium",
    "description": "Com Cardapium você não precisa se preocupar com o que fazer para comer",
    "authors": [
        {
            "name": "Marcio Satin",
            "email": "marcio.satin@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "robmorgan/phinx": "0.7.1",
        "xtreamwayz/pimple-container-interop": "^1.0",
        "aura/router": "3.1.0",
        "laminas/laminas-diactoros": "2.17.0",
        "laminas/laminas-httphandlerrunner": "2.1.0",
        "twig/twig": "~1.0",
        "illuminate/database": "6.20.26",
        "jasny/auth": "1.0.1",
        "vlucas/phpdotenv": "2.4",
        "laminas/laminas-validator": "2.16.0",
        "laminas/laminas-filter": "2.14.0"
    },
    "autoload": {
        "psr-4": {
            "Cardapium\\": "src/Cardapium/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "vendor/bin/phinx migrate"
        ]
    }
}
